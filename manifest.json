{
  "manifest_version": 2,
  "name" : "DLNA connector for YouTube",
  "version" : "0.1",
  "background": {
    "scripts": ["/javascripts/libs/jquery-1.9.1.min.js",  "/javascripts/change_header.js"]
  },
  "permissions" : [
      "webRequest",
      "webRequestBlocking",
      "*://m.youtube.com/*",
      "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
